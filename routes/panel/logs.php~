<?php

use Illuminate\Support\Facades\Route;

Route::get('/',
    [App\Http\Controllers\Admin\LogsController::class, 'index'])
    ->can('admin', 'App\Models\User')
    ->name('admin.ip-filter');

Route::post('/',
    [App\Http\Controllers\Admin\LogsController::class, 'logsData'])
    ->can('admin', 'App\Models\User')
    ->name('admin.ip-filter');
